   "source": [
    "class Solution(object):\n",
    "    def sortColors(self, nums):\n",
    "        for i in range(len(nums)):\n",
    "            for j in range(len(nums)-1):\n",
    "                if(nums[i]<nums[j]):\n",
    "                    temp=nums[i]\n",
    "                    nums[i]=nums[j]\n",
    "                    nums[j]=temp\n"
   ]
  }
